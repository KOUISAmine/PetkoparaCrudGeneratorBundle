{
    "name": "Triton/crud-generator",
    "description": "Flexible bundle for CRUD generation",
    "keywords": ["crud", "generator"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Petko Petkov",
            "email": "petkopara@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": ">=2.8.0",
        "sensio/generator-bundle": ">=3.0.0",
        "pagerfanta/pagerfanta": "1.0.*@dev",
        "lexik/form-filter-bundle": "~5.0",
        "doctrine/orm": ">=2.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Triton\\Bundle\\CrudBundle": ""
        }
    },
    "target-dir": "Triton/Bundle/CrudBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
